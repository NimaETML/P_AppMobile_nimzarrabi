<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ReadMe_Nima_Zarrabi.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ReadMe_Nima_Zarrabi"
    Shell.FlyoutBehavior="Flyout"
    Shell.TabBarIsVisible="True"
    Title="ReadMe_Nima_Zarrabi">

    <Tab Title="Home" x:Name="HomeTab">
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:HomePage}" 
        Route="HomePage"/>
    </Tab>

    <Tab Title="ApiPage">
        <ShellContent
    Title="ApiPage"
    ContentTemplate="{DataTemplate local:ApiPage}" 
    Route="ApiPage"/>
    </Tab>

    <Tab Title="ReadPage">
        <ShellContent
    Title="ReadPage"
    ContentTemplate="{DataTemplate local:ReadPage}" 
    Route="ReadPage"/>
    </Tab>

</Shell>
